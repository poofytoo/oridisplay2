     40          /**
     41            * @brief  This function return a file extension
     42            * @param  filename : full file name string
     43            * @retval File extension string
     44            */
     45          char *get_filename_ext(const char *filename) 
     46          { 
     47            char *dot = strrchr(filename, '.'); 
     48            if(!dot || dot == filename) return ""; 
     49            return dot + 1; 
     50          } 
     51          /**
     52            * @brief  This function check file extension
     53            * @param  filename : full file name string
     54            * @param  ext : file extension string
     55            * @retval result : true or false
     56            */
     57          uint8_t check_filename_ext (char *filename, char *ext)
     58          {
     59            char *temp;
     60            temp = get_filename_ext (filename);
     61            
     62            return !strcmp (temp, ext);
     63            
     64          }
