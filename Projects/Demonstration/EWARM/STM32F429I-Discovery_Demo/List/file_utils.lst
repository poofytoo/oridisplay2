     41          /**
     42            * @brief  Retieve the parent directory from a full file path
     43            * @param  path: pointer to file path
     44            * @retval None
     45          */
     46          void GetParentDir (char *dir)
     47          {
     48            uint8_t idx = FILEMGR_DIRECTORYSIZE;
     49            
     50            for (  ; idx > 0 ; idx --)
     51            {
     52              
     53              if (dir [idx] == '/')
     54              {
     55                dir [idx] = 0;
     56                break;
     57              }
     58              dir [idx] = 0;
     59            }
     60          }
